<%= render "shared/pagestart" %>
  <div class="container body-container">
    <% flash.each do |key, value| -%>
        <% if value.present? %>
          <div class="alert alert-<% key %>">
            <button type="button" class="close" data-dismiss="alert">&times;</button>
            <%= flash[key] %>
          </div>
        <% end %>
    <% end -%>
    
    <%= yield %>
  </div>
<%= render "shared/pageend" %>