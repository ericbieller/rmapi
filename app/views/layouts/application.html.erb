<%= render "shared/pagestart" %>
  <div class="container body-container">
    <% flash.each do |key, value| %>
    <div class="alert alert-error">
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      <%= value %>
    </div>
    <% end %>
    
    <%= yield %>
  </div>
<%= render "shared/pageend" %>